//declare and print largest and smallest 5 integers 
#include<stdio.h>
int main()
{
    int arr[5];
    printf("Enter 5 integers:- \n");
    for(int i = 0 ; i<5 ; i++)
    {
        scanf("%d" , &arr[i]);
    }
    int max = arr[0];
    for(int i = 0 ; i<5 ; i++)
    {
        if(arr[i]>max)
        {
            max = arr[i];
        }
    }
    int min = arr[0];
    for(int i = 0 ; i<5 ; i++)
    {
        if(arr[i]<min)
        {
            min = arr[i];
        }
    }
    printf("The largest and smallest integers among the provided intgers are %d and %d respectively." , max , min);
    return 0;
}
