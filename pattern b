// pattern b
#include <stdio.h>
int main()
{
    int n;
    printf("Enter the value of n:- ");
    scanf("%d", &n);
    for (int i = 1; i <= n; i++) // for rows
    {
        for (int j = 1; j <= i; j++)// for number of stars in that row
        {
            printf("%d " , j);
        }
        printf("\n");
    }
    return 0;
}
