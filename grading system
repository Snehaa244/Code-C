//grading system
#include<stdio.h>
int main()
{
    float per;
    printf("Enter your percentage:-\n");
    scanf("%f" , &per);
    if(per<5.0)
    {
    printf("Fail");
    }
    else if (per>=5.0&&per<5.5)
    {
        printf("Third Division");
    }
    else if (per>=5.5&&per<6.5)
    {
        printf("Second Division");
    }
    else if (per>=6.5&&per<7.5)
    {
        printf("First Division");
    }
    else 
    {
        printf("Distinction");
    }
    
    return 0;
}
