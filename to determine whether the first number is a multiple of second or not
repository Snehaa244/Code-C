//to determine whether the first number is a multiple of second or not
#include<stdio.h>
int main()
{
    int num1 , num2;
    printf("Enter two number (num1 should be greater than num2):-\n");
    scanf("%d%d" , &num1 , &num2);
    if(num1%num2==0)
    {
        printf("%d is a multiple of %d" ,num1,num2);
    }
    else
    {
        printf("%d is not a multiple of %d" ,num1,num2);
    }
    return 0;
}
