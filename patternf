//pattern f
#include <stdio.h>
int main()
{
    int n;
    printf("Enter the value of n:- ");
    scanf("%d" , &n);
    for(int i=n ; i>=1 ; i--)//for rows
    {
        for(int j=1 ; j<=i ; j++)
        {
            printf("%c " , (char)(j+64));
        }
        printf("\n");
    }
    return 0;
}
