//to find gcd and lcm
#include<stdio.h>
int main()
{
    int a,b,temp=0,gcd,lcm,num1,num2;
    printf("Enter two numbers:- \n");
    scanf("%d%d" , &num1,&num2);
    while (b!=0)
    {
        temp = b;
        b=a%b;
        a=temp;
    }
    gcd = a;
    lcm=(num1*num2)/gcd;
    printf("The gcd and lcm of %d and %d is %d and %d respectively." , num1,num2,gcd,lcm);
    
    return 0;
}
