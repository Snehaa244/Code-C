/*if the first num is lesser , prog will print up ; if the first number is greater prog will print down , if they are equal , prog will print equal 
else it will return error
*/
#include<stdio.h>
int main()
{
    int num1 , num2;
    printf("Enter two numbers: ");
    scanf("%d%d" , &num1 , &num2);
    if(num1<num2)
    printf("up");
    else if (num1>num2)
    printf("down");
    else if(num1==num2)
    printf("Equal");
    else
    printf("Error!");
    return 0;
}
