//swap with third variable
#include<stdio.h>
int main()
{
    int x , y , temp=0;
    printf("Enter first number : ");
    scanf("%d" , &x);
    printf("Enter second number : ");
    scanf("%d" , &y);
    temp = x;
    x= y;
    y = temp;
    printf("After swapping:\n");
    printf("Now the value of first number is : %d\n" , x);
    printf("Now the value of second number is : %d\n" , y);
    return 0;
}
