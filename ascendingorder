//three numbers in ascending order using if else
#include<stdio.h>
void print()
{
     printf("numbers in ascending order:-\n");
}
int main()
{
    int n1,n2,n3;
    printf("Enter three numbers:-\n");
    scanf("%d%d%d" , &n1,&n2,&n3);
    if(n1>n2&&n1>n3)
    {
        if(n2>n3)
        {
           print();
           printf("%d\t%d\t%d" , n3 , n2 , n1); 
        }
        else
        {
            print();
            printf("%d\t%d\t%d" , n2 , n3 , n1); 
        }
        
    }
    else if(n2>n1&&n2>n3)
    {
        if(n1>n3)
        {
           print();
           printf("%d\t%d\t%d" , n3 , n1 , n2); 
        }
        else
        {
            print();
            printf("%d\t%d\t%d" , n1 , n3 , n2); 
        }
        
    }
    else
    {
        if(n1>n2)
        {
           print();
           printf("%d\t%d\t%d" , n2 , n1 , n3); 
        }
        else
        {
            print();
            printf("%d\t%d\t%d" , n3 , n2 , n1); 
        }
        
    }


    return 0;
}
